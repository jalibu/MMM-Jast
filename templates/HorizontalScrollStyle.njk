{% macro render() %}
  <style type="text/css">
    .jast-wrapper.horizontal ul.jast-tickerframe {
      animation: tickerh {{ config.fadeSpeedInSeconds }}s linear infinite;
    }

    .jast-wrapper.horizontal .jast-ticker-no2 ul.jast-tickerframe {
      animation-delay: -{{ config.fadeSpeedInSeconds / 2 }}s;
    }
  </style>
{% endmacro %}
